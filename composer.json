{
    "name": "pass-qa/delivery",
    "description": "Pass delivery service API library",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PassQa\\Delivery\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PassQa\\Delivery\\Library\\Tests\\": "library/tests"
        }
    },
    "authors": [
        {
            "name": "Pass - Instant Delivery Platform",
            "email": "techsuport@gmail.com"
        },
        {
            "name": "Mostafa sharami",
            "email": "mostafa.sharami@gmail.com",
            "role": "Senior Software Developer"
        },
        {
            "name": "Majva",
            "email": "m.vahidkhoo@gmail.com",
            "role": "Senior Software Developer"
        }
    ],
    "require": {
        "ext-curl": "*",
        "ext-json": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "PassQa\\Delivery\\Providers\\PassDeliveryApiServiceProvider"
            ],
            "aliases": {
                "PassOrder": "PassQa\\Delivery\\Facades\\PassOrder"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^8.0"
    }
